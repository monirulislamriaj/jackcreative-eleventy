{% extends "base.njk" %}
{% block navbar %}{% endblock %}
  {% block content %}
    <section class="error py-xxl-12 py-lg-9 py-10 py-sm-6">
      <div class="error-shape-wrapper position-absolute w-100 h-100">
        <div class="shape-1"></div>
        <div class="shape-2"></div>
      </div>
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="error-content text-center">
              <div class="logo mb-7">
                <a class="navbar-brand" href="/{{btnLink}}">
                  <img src="{{navbar.logo}}" alt="" class="" />
                </a>
              </div>
              <img src="{{img}}" class="" alt="error-image" />
              <h3 class="my-5">{{error}}</h3>
              <a href="/{{btnLink}}" class="btn btn-lg btn-primary">
                <span class="position-relative">
                  {{btnText}}
                  <i class="ph-arrow-up-right"></i
                ></span>
                <span class="position-relative">
                  {{btnText}}
                  <i class="ph-arrow-up-right"></i
                ></span>
              </a>
            </div>
          </div>
        </div>
      </div>
    </section>
  {% endblock %}
{% block footer %}{% endblock %}